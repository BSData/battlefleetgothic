            <profile id="ad93-9998-658e-56b1" name="Retaliator Starboard Launch Bays" hidden="false" profileTypeId="41726d616d656e7423232344415441232323" profileTypeName="Armament">
              <profiles/>
              <rules/>
              <infoLinks/>
              <modifiers/>
              <characteristics>
                <characteristic name="Range/Speed" characteristicTypeId="52616e67652f537065656423232344415441232323" value="Swiftdeaths: 30cm Doomfires: 20cm Dreadclaws: 30cm"/>
                <characteristic name="Firepower/Str" characteristicTypeId="46697265706f7765722f53747223232344415441232323" value="3 Squadrons"/>
