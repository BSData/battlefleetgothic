              </constraints>
              <rules>
                <rule id="346e-7f24-602d-533c" name="Legacies of Old Night" hidden="false">
                  <description>Dark Angels may take one less cruiser/light cruiser before they are allowed to take a heavy/battle/grand cruiser. So instead of a 1:2 ratio, The Dark Angels may take them at 1:1.</description>
                </rule>
              </rules>
              <costs>
                <cost name="pts" typeId="points" value="0.0"/>
              </costs>
            </selectionEntry>
            <selectionEntry id="0b4f-d2f5-c6d4-86df" name="Death Guard" hidden="false" collective="false" import="true" type="upgrade">
