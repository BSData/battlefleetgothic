                          <profiles/>
                          <rules/>
                          <infoLinks/>
                          <modifiers/>
                          <description>The vessel has been totally and completely dedicated to the Blood God, Khorne, who values and rewards those who spill blood (and whose blood is spilled) in his name. The marked vessel always doubles its boarding value.</description>
                        </rule>
