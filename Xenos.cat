    <selectionEntry id="359f-8331-692f-4b7d" name="Prince Yriel, Bearer Of The Flame, Autarch Of Iyandea" publicationId="11f0-17d1-e4d2-1018" page="137" hidden="false" collective="false" import="true" type="upgrade">
      <modifiers>
        <modifier type="decrement" field="points" value="25">
          <conditions>
            <condition field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" childId="d358-7438-e160-90de" type="instanceOf"/>
