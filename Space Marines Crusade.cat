                                                                <selectionEntry id="35a1-c9e7-fcce-167a" name="Mark of Slaanesh" page="0" hidden="false" collective="false" type="upgrade">
                                                                                                      
                                                                    <rules>
                                                                                                            
                                                                        <rule id="ed2f-e429-4952-629c" name="Mark of Slaanesh" hidden="false">
                                                                                                                  
                                                                            <description>The ship is full of the sensation-craving followers of Slaanesh and their siren cries extend into the minds of the crews of nearby enemy ships. Enemy ships within 15cm suffer â2 to their Leadership value.</description>
                                                                                                                
                                                                        </rule>
                                                                                                          
                                                                    </rules>
                                                                                                      
