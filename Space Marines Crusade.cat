                                                                <selectionEntry id="a645-0207-67c2-8562" name="Mark of Khorne" page="0" hidden="false" collective="false" type="upgrade">
                                                                                                      
                                                                    <rules>
                                                                                                            
                                                                        <rule id="7509-3106-30d8-03e7" name="Mark of Khorne" hidden="false">
                                                                                                                  
                                                                            <description>Crewed by the homicidal followers of Khorne, the ship is extremely dangerous in boarding actions. It doubles its value in boarding actions.</description>
                                                                                                                
                                                                        </rule>
                                                                                                          
                                                                    </rules>
                                                                                                      
                                                                    <costs>
                                                                                                            
                                                                        <cost name="pts" typeId="points" value="20.0"/>
                                                                                                          
