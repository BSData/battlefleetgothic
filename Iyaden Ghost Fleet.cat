                                <profile id="0fd1-a8d7-a446-c139" name="Keel Launch Bay" hidden="false" typeId="41726d616d656e7423232344415441232323" typeName="Armament">
                                                      
                                    <characteristics>
                                                            
                                        <characteristic name="Range/Speed" typeId="52616e67652f537065656423232344415441232323">Nightwing: 30cm Phoenix: 20cm</characteristic>
                                                            
                                        <characteristic name="Firepower/Str" typeId="46697265706f7765722f53747223232344415441232323">4 Squadrons</characteristic>
                                                            
                                        <characteristic name="Fire Arc" typeId="466972652041726323232344415441232323">-</characteristic>
                                                          
